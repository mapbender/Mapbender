{
    "name": "mapbender/mapbender",
    "description": "Mapbender library",
    "keywords": ["mapbender", "module"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/symfony": "^4.3",
        "symfony/security-acl": "~2.8 || ~3.0",
        "symfony/acl-bundle": "^1 || ^2",
        "twig/twig": "^2.13",
        "doctrine/orm": "^2.10",
        "doctrine/dbal": "^2.11 || ^3",
        "doctrine/doctrine-bundle": "^1.12 || ^2",
        "symfony/swiftmailer-bundle": "^2.3 || ^3",
        "sensio/framework-extra-bundle": "^3 || ^4 || ^5 || ^6",
        "shuchkin/simplexlsxgen": "^1.3",

        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2",

        "assetic/framework": "^2.0",

        "wheregroup/compass-mixins": "0.x",
        "mapbender/mapbender-icons": "^1.3",

        "wheregroup/codemirror": "5.x",
        "components/underscore": "1.x",
        "wheregroup/doctrine-dbal-shims": "^1",
        "wheregroup/assetic-filter-sassc": "^0.0",
        "wheregroup/sassc-binaries": "^0.0.1",

        "mapbender/owsproxy": "^3.1.8",

        "datatables/datatables": "^1.10.16",
        "components/bootstrap": "^3 || ^4",
        "components/jqueryui": "^1",

        "mapbender/openlayers6-es5": "^0.4.3.2 || ^6.4.3.2 || ^7",
        "mapbender/mapquery": "1.x"
    },
    "provide": {
        "mapbender/fom": "3.3",
        "mapbender/owsproxy": "3.3.99"
    },
    "replace": {
        "mapbender/owsproxy": "3.3.*",
        "mapbender/fom": "*"
    },
    "autoload": {
        "psr-4": {
            "Mapbender\\": "src/Mapbender",
            "OwsProxy3\\": "src/OwsProxy3",
            "FOM\\": "src/FOM"
        }
    },
    "conflict": {
        "mapbender/digitizer": "<1.4.8",
        "mapbender/data-manager": "<1.1",
        "mapbender/data-source": "<0.1.11",
        "mapbender/coordinates-utility": "<1.0.9",
        "wheregroup/jquerydialogextendjs": "<2.1",
        "mapbender/vis-ui.js": "<0.1.80",
        "mapbender/wmts": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-release/3.0.5": "3.0.5.x-dev",
            "dev-release/3.0.6": "3.0.6.x-dev",
            "dev-release/3.0.7": "3.0.8.x-dev",
            "dev-release/3.2": "3.2.x-dev",
            "dev-master": "3.3.x-dev"
        }
    }
}
