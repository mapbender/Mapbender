<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Mapbender\FrameworkBundle\Command\ListSourcesCommand"
                 class="Mapbender\FrameworkBundle\Command\ListSourcesCommand"
                 lazy="true">
            <tag name="console.command" command="mapbender:sources:list" />
            <argument type="service" id="doctrine" />
        </service>
        <service id="Mapbender\FrameworkBundle\Command\ReloadSourcesCommand"
                 class="Mapbender\FrameworkBundle\Command\ReloadSourcesCommand"
                 lazy="true">
            <tag name="console.command" command="mapbender:sources:reload" />
            <argument type="service" id="doctrine" />
            <argument type="service" id="mapbender.source.typedirectory.service" />
        </service>
    </services>
</container>
