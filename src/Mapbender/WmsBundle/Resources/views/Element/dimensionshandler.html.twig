{% for dimension_config in dimensionsets %}
  <div class="dimensionset row no-gutters"
       data-group="{{ dimension_config.group | join('#') }}"
       data-extent="{{ dimension_config.extent }}">
    <div class="col-xs-4 col-lg-3 dimensionset-title">{{ dimension_config.title }}</div>
    <div class="col-xs-4 col-lg-6 slider-wrap"><div class="mb-slider"></div></div>
    <div class="col-xs-4 col-lg-3 dimensionset-value"></div>
  </div>
{% endfor %}
