<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="mapbender.print.storage_dir">prints</parameter>
        <parameter key="mapbender.print.max_age">3</parameter>
        <parameter key="mapbender.print.priority_voter.class">Mapbender\PrintBundle\Component\IdentityPriorityVoter</parameter>
        <parameter key="mapbender.print.queue_manager.class">Mapbender\PrintBundle\Component\PrintQueueManager</parameter>
    </parameters>

    <services>
        <service class="Mapbender\PrintBundle\Component\PrintService" id="mapbender.print.engine">
            <argument type="service" id="service_container" />
        </service>
        <service id="mapbender.print.queue_manager" class="%mapbender.print.queue_manager.class%">
            <argument type="service" id="service_container" />
            <argument type="service" id="mapbender.print.priority_voter"/>
        </service>
        <service id="mapbender.print.priority_voter" class="%mapbender.print.priority_voter.class%"></service>
    </services>
</container>

